<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cardView"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_gravity="center"
    android:layout_margin="1dp"
    card_view:cardCornerRadius="4dp"
    card_view:cardElevation="0dp"
    android:clickable="true"
    card_view:cardMaxElevation="1dp">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="@dimen/padding5">


        <TextView
            android:id="@+id/accessIdTV"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="5dp"
            android:text="Access Id :"
            android:textColor="@color/black"
            card_view:layout_constraintStart_toStartOf="parent"
            card_view:layout_constraintTop_toTopOf="parent" />
        <TextView
            android:id="@+id/accessValTV"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="5dp"
            android:layout_marginRight="5dp"
            android:text="0"
            android:textColor="@color/red"
            card_view:layout_constraintStart_toEndOf="@id/accessIdTV"
            card_view:layout_constraintTop_toTopOf="@+id/accessIdTV" />

        <TextView
            android:id="@+id/codeTV"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="4dp"
            android:layout_marginRight="4dp"
            android:text="Branch :"
            android:textColor="@color/black"
            card_view:layout_constraintEnd_toEndOf="parent"
            card_view:layout_constraintTop_toTopOf="@+id/accessIdTV" />
        <TextView
            android:id="@+id/branchTV"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="5dp"
            android:layout_marginRight="5dp"
            android:text="78M"
            android:textColor="@color/red"
            card_view:layout_constraintStart_toEndOf="@+id/codeTV"
            card_view:layout_constraintTop_toTopOf="@+id/codeTV"
            android:visibility="gone"/>

        <TextView
            android:id="@+id/nameTV"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="2dp"
            android:layout_marginLeft="2dp"
            android:layout_marginTop="8dp"
            android:text="Customer Name :"
            android:textColor="@color/black"
            card_view:layout_constraintStart_toStartOf="parent"
            card_view:layout_constraintTop_toBottomOf="@+id/accessIdTV" />
        <TextView
            android:id="@+id/nameValTV"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="5dp"
            android:layout_marginRight="5dp"
            android:text="0"
            android:textColor="@color/red"
            card_view:layout_constraintStart_toEndOf="@+id/nameTV"
            card_view:layout_constraintTop_toTopOf="@+id/nameTV" />
        <TextView
            android:id="@+id/textView40"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginRight="16dp"
            android:text="Proposal No. : "
            android:layout_marginLeft="2dp"
            android:textColor="@color/black"
            android:layout_marginTop="@dimen/margin5"
            card_view:layout_constraintStart_toStartOf="parent"
            card_view:layout_constraintTop_toBottomOf="@+id/nameTV"/>
        <TextView
            android:id="@+id/proposalTV"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="5dp"
            android:layout_marginRight="5dp"
            android:text="0"
            android:textColor="@color/red"
            card_view:layout_constraintStart_toEndOf="@id/textView40"
            card_view:layout_constraintTop_toTopOf="@+id/textView40" />

        <TextView
            android:id="@+id/textView35"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="S.U.C :"
            android:textColor="@color/black"
            card_view:layout_constraintEnd_toStartOf="@+id/sucTV"
            card_view:layout_constraintTop_toTopOf="@+id/textView40" />
        <TextView
            android:id="@+id/sucTV"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="5dp"
            android:layout_marginRight="5dp"
            android:text="10000"
            android:textColor="@color/red"
            card_view:layout_constraintEnd_toEndOf="parent"
            card_view:layout_constraintTop_toTopOf="@+id/textView40" />




    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.cardview.widget.CardView>